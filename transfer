def taux_infl_12m(res):
    res.sort_values(by=["Periode"])
    if res[res["Periode"] == res["Periode"].year.min()]:
        return np.nan
    else :
        for i in res["Periode"]
        return (res["prix_mm_value"].iloc[i] - res["prix_mm_value"].iloc[i - 1]) / res["prix_mm_value"].iloc[i - 1])
res.apply(taux_infl_12m)
